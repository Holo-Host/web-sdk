{
  "name": "@holo-host/web-sdk",
  "version": "0.1.0",
  "description": "Standard development kit for creating Web UIs that work with Holo Hosting.",
  "main": "build/index.js",
  "module": "build/index.js",
  "browser": "dist/holo_hosting_web_sdk.js",
  "scripts": {
    "build": "npm run compile && npm run bundle",
    "compile": "tsc --esModuleInterop --lib es2015,dom --outDir ./build -p ./src",
    "bundle": "npx webpack --mode production --output-filename holo_hosting_web_sdk.js",
    "test": "npm run test-unit",
    "test-unit": "mocha ./tests/unit/"
  },
  "author": "Holo Inc.",
  "contributors": [
    "Matthew Brisebois <matthew.brisebois@holo.host>"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Holo-Host/web-sdk/"
  },
  "files": [
    "README.md",
    "dist/*",
    "build/*"
  ],
  "dependencies": {
    "@holo-host/comb": "^0.1.3"
  },
  "devDependencies": {
    "@types/node": "^12.11.1",
    "@whi/stdlog": "^0.3.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "puppeteer": "^2.1.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  }
}
