{
  "name": "@holo-host/web-sdk",
  "version": "0.3.1",
  "description": "Standard development kit for creating Web UIs that work with Holo Hosting.",
  "main": "src/index.js",
  "module": "src/index.js",
  "scripts": {
    "build": "npm run bundle",
    "bundle": "npx webpack --mode production --output-filename holo_hosting_web_sdk.js",
    "test": "npm run test-unit",
    "test-unit": "mocha ./tests/unit/",
    "start-static-chaperone": "cd chaperone/dist && npx simplehttpserver --mime.application/wasm,wasm .",
    "stop-static-chaperone": "kill -9 $(lsof -t -i:8000)",
    "start-chaperone": "npx chaperone-server --config ./chaperone-dev-server/chaperone.json",
    "stop-chaperone": "kill -9 $(lsof -t -i:24273)",
    "start-websdk": "python -m SimpleHTTPServer 8080",
    "stop-websdk": "kill -9 $(lsof -t -i:8080)"
  },
  "author": "Holo Ltd.",
  "contributors": [
    "Matthew Brisebois <matthew.brisebois@holo.host>"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Holo-Host/web-sdk/"
  },
  "files": [
    "README.md",
    "dist/*",
    "src/*"
  ],
  "dependencies": {
    "@holo-host/chaperone": "Holo-Host/chaperone#B-03916-first-time-login",
    "@holo-host/comb": "^0.2.0"
  },
  "devDependencies": {
    "@holo-host/mock-conductor": "^0.3.0",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@types/node": "^12.11.1",
    "@whi/stdlog": "^0.3.0",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "mocha": "^6.2.1",
    "puppeteer": "^2.1.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.12"
  }
}
